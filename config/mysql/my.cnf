[client]
port=3306
socket=/var/run/mysql/mysql.sock
ssl-ca=/var/lib/mysql-files/ca.pem
ssl-key=/var/lib/mysql-files/client-key.pem
ssl-cert=/var/lib/mysql-files/client-cert.pem
default-character-set=utf8
[mysqld]
user=mysql
port=3306
basedir=/var/lib/mysql/
datadir=/var/lib/mysql/
pid-file=/var/run/mysql/mysql.pid
socket=/var/run/mysql/mysql.sock
ssl-ca=/var/lib/mysql-files/ca.pem
ssl-key=/var/lib/mysql-files/server-key.pem
ssl-cert=/var/lib/mysql-files/server-cert.pem
lc-messages-dir=/usr/share/mysql/
sql_mode=NO_ENGINE_SUBSTITUTION,STRICT_TRANS_TABLES,NO_ZERO_DATE,NO_ZERO_IN_DATE,ERROR_FOR_DIVISION_BY_ZERO,NO_AUTO_CREATE_USER,ONLY_FULL_GROUP_BY
secure-file-priv=/var/lib/mysql-files/
key_buffer_size=16M
max_allowed_packet=1M
table_open_cache=64
sort_buffer_size=512K
net_buffer_length=8K
read_buffer_size=256K
read_rnd_buffer_size=512K
myisam_sort_buffer_size=8M
symbolic-links=0
innodb_flush_log_at_trx_commit=2
character-set-server=utf8
collation-server=utf8_general_ci
skip-external-locking=true
explicit_defaults_for_timestamp=true
disable-partition-engine-check=true
skip-host-cache
log_timestamps=system
!includedir=/etc/mysql/
